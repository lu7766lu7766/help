// 將您的設定放入此檔案中以覆寫預設值和使用者設定。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.ttf": true,
        "**/*.eot": true,
        "**/*.woff": true,
        "**/*.woff2": true,
        "**/*.svg": true,
        "**/*.min.js": true,
        "src/vendor.js": true,
        "LICENSE": true
    },
    "files.associations": {
        "*.vue": "vue"
    },
    /* Format on save using */
    "formatOnSave.on": true,
    /* The extensions array which will apply */
    "formatOnSave.extensions": ["*"]
}